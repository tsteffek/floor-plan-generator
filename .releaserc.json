{
  "branches": [
    "master",
    {
      "name": "dev*",
      "prerelease": true
    },
    {
      "name": "beta",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@semantic-release/github", {
      "assets": [
        {"path": "build/distributions/*.{zip,tar}", "label": "contains Windows and Unix start scripts"}
      ]
    }]
  ]
}
